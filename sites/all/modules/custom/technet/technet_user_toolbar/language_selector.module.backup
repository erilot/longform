/**
 * Implements hook_block_view().
 */
function language_selector_block_view($delta) {
  
  $node = menu_get_object();
  
  switch($delta){
    
    case 'language_selector':
      //language selector      
      
      $key_language = $node->tnid;
      if(!$key_language){$key_language = $nodeid;}
      $current_language = $node->language;
      
      $other_languages = get_node_translations($key_language, $current_language);
              
      $output = '<ul id="menu-icons">';

      $output .= '<li id="switch-language" class="menu-icon">' . language_selector_ls_output($node) . '</li>';
      $output .= '</ul>';
      
      //define block 
      $blocks['subject'] = t('Language Selector');
      $blocks['content'] = $output;
    
    break;
    
    case 'navigation_help':
      //help button      
      $output = '<ul id="navigation-help">';

      $output .= '<li id="ui-help" class="menu-icon">' . language_selector_help_output($node) . '</li>';
      $output .= "</ul>";
      
      //define block
      $blocks['subject'] = t('Navigation Help');
      $blocks['content'] = language_selector_help_output($node);
    break;
  
    case 'print_options':
      //print function
      
      $output = '<ul id="print-options">';
      $output .= '<li id="ui-print" class="menu-icon">' . language_selector_print_output($node) . '</li>';
      $output .="</ul>";
      $blocks['subject'] = t('Print Options');
      $blocks['content'] = $output;
    break;
  }  
      
  return $blocks;

}

// HTML Builder Functions

function language_selector_ls_output($node){
  
  drupal_add_library('system', 'drupal.ajax');
  
  //translatable strings
  $block_title = t("Languages");
  $formal_translation_title = t("Formal Translations");
  $machine_translation_title = t("Machine Translations");
  
  $formal_translation_options = get_node_translations($node->nid, $node->language);
  
  if($formal_translation_options){
    $formal_block =
    '<div id="formal-translations" class="translation-block menu-drop-options"><h3>'.$formal_translation_title.'</h3>
      <ul class="formal-options">' . $formal_translation_options . '</ul></div>';
  }
  else{
    $formal_block = '';
  }

  //NOTE: future AJAX implementation of this menu should use this link: <a href="'.base_path().'language_selector/ajax" class="use-ajax">...</a>
  //build HTML
  $ls_output = '<div id="language-selector" class="menu-bar-icons menu-drop-outer"><div class="arrow">&nbsp;</div><div class="arrow-border">&nbsp;</div>';
  $ls_output .= '<div id="language-selector-inner" class="menu-drop-inner"><h2>'.$block_title.'</h2>';
  $ls_output .= '<div id="language-options" class="menu-drop-options-container">';
  $ls_output .=  $formal_block;
  $ls_output .= '<div id="machine-translations" class="translation-block menu-drop-options"><h3>'.$machine_translation_title.'</h3>';
  
  //if GT cookie exists, don't load disclaimer
  if (!$_COOKIE['google-translate-ok']){
    $ls_output .= '<ul class="machine-options"><li id="gt-disclaimer">' . gt_disclaimer() . '</a></li> </ul>'. gt_widget();
  }
  else{
    $ls_output .= gt_widget();
  }
  
  $ls_output .='</div></div></div></div>';

  return $ls_output;
}

function language_selector_ajax_gt_callback($nid = 26126) {
  $node = node_load($nid);
  
  $output = $node->body[LANGUAGE_NONE][0]['value'];
  $commands = array(); // define the div id to return output.
  $commands[] = ajax_command_html('#gt-disclaimer', $output);

  $response = array('#type' => 'ajax', '#commands' => $commands);
  //$response = "<h1>HI MOM</h1>";
  dpm($response,'response');
  ajax_deliver($response);

}

function get_real_names($lang_code){
  switch ($lang_code){
    case 'en': $lang = t("English"); break;
    case 'de': $lang = t("German"); break;
    case 'es': $lang = t("Spanish"); break;
    case 'fr': $lang = t("French"); break;
    case 'it': $lang = t("Italian"); break;
    case 'ko': $lang = t("Korean"); break;
    case 'hu': $lang = t("Hungarian"); break;
    case 'no': $lang = t("Norwegian"); break;
    case 'po': $lang = t("Polish"); break;
    case 'ru': $lang = t("Russian"); break;
    default:    $lang = t("English"); break;
  }
  return $lang;
}

function get_node_translations($nodeid, $current_language){
  $translations = translation_node_get_translations($nodeid);
  $language_set = array();
  if(!empty($translations)){
    foreach($translations as $language){
      if ($language->language != $current_language){
        $language_set[] = array('language' => $language->language, 'nid' => $language->nid);
      }
    }
  }
    if ($current_language != "en"){
      $language_set[] = array('language' => "en", 'nid' => node_load($language->nid)->tnid);
    }
  
  if(count($language_set) != 0){
    foreach ($language_set as $element){
      //dpm($element['nid'],'NID');
        $list .= '<li><a href="node/'. $element['nid'] . '">' . get_real_names($element['language']) . '</a></li>';
      }
      //dpm($list,'list');
    return $list;
  }
  else{
    return FALSE;
  }
}

